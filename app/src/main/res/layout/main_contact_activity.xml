<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#e0e0e0"
    tools:context="com.bisa.health.MainActivity" >

    <com.bisa.health.cust.view.ActionBarView
        app:isNext="true"
        app:nextIco="@drawable/menu_add_dev"
        app:isPre="true"
        app:preIco="@drawable/menu_back_bg"
        app:title="@string/title_sos_contact_user"
        android:id="@+id/abar_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <RelativeLayout
        android:layout_below="@+id/abar_title"
        android:id="@+id/rl_tip"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:layout_marginTop="10dp"
            android:id="@+id/tv_tip"
            android:layout_centerHorizontal="true"
            android:text="@string/msg_data"
            android:textSize="14sp"
            android:layout_marginBottom="5dp"
            android:textColor="@color/color_login_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <View
            android:id="@+id/view_line"
            android:layout_marginLeft="80dp"
            android:layout_marginRight="80dp"
            android:background="@color/color_login_type"
            android:layout_below="@+id/tv_tip"
            android:layout_width="match_parent"
            android:layout_height="1dp"/>
    </RelativeLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/abar_title"
        android:layout_marginTop="2dp"
        android:orientation="vertical" >


        <ListView
            android:id="@+id/list_contact"
            android:layout_width="match_parent"
            android:layout_height="match_parent">


        </ListView>

    </LinearLayout>

</RelativeLayout>