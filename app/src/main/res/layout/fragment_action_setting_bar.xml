<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/color_default_bg"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context="com.bisa.health.cust.view.ActionBarView">
    
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="47dp">

        <RelativeLayout
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:id="@+id/rl_back"
            android:layout_width="100dp"
            android:layout_height="match_parent">
            <ImageButton
                android:clickable="false"
                android:padding="20dp"
                android:layout_marginLeft="14dp"
                android:layout_centerVertical="true"
                android:id="@+id/ibtn_back"
                android:background="@drawable/menu_back_bg"
                android:layout_width="30dp"
                android:layout_height="30dp" />

        </RelativeLayout>


        <TextView
            android:id="@+id/tv_title"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="16sp"
            android:textColor="@color/color_search_title"
            tools:text="中间位置"
            android:text="" />

        <RelativeLayout
            android:id="@+id/rl_next"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true">

            <ImageButton
                android:clickable="false"
                android:layout_marginRight="14dp"
                android:id="@+id/ibtn_next"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@drawable/menu_back_setting_bg" />

        </RelativeLayout>

    </RelativeLayout>
    


</FrameLayout>
